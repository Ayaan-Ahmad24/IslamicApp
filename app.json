{
  "expo": {
    "name": "islamic_app",
    "slug": "islamic_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/OBJECTS.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/OBJECTS.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/OBJECTS.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.ayaanahmad.islamic_app"
    },
    "web": {
      "favicon": "./assets/icons/OBJECTS.png"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a8bfee30-b142-4446-8a20-1852c575065c"
      }
    }
  }
}
